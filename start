termux-setup-storage -y
cd /sdcard
mkdir woltrex
cd woltrex
mkdir .tools
cd .tools
git clone https://github.com/woltrex22/woltrex-torrent-downloader
cd /sdcard/woltrex
mkdir torrents
apt update -y
clear
apt upgrade -y
clear
apt install aria2 -y
clear
printf 'Please Wait Loading...'
sleep 5s
clear
cd /sdcard/woltrex/.tools/woltrex-torrent-downloader/.bin
unzip -o -p .woltrex woltrex
sh torrent

